#include<iostream>
using namespace std;
void printdec(int n)
{
   //1.BC 
if(n==0)
{
    return ;
}
    //2.RF
    
    cout<<n<<" ";
    printdec(n-1);
    
    
    
}
void printinc(int n)
{
   //1.BC 
if(n==0)
{
    return ;
}
    //2.RF
    
   
    printinc(n-1);
     cout<<n<<" ";
    
    
}
int main()
{
    int n;
    cin>>n;
    printdec(n);
printinc(n);
    return 0;
}
